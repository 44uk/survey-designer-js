{
  "extends": "airbnb",
  "rules": {
    "max-len": ["error", 140, 2, {
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignoreRegExpLiterals": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true
    }],
    "no-plusplus": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  }
}
